SELECT
			T.WORK_SEQ AS "WORK_SEQ"
			,T.TASK_SEQ AS "TASK_SEQ"
			,T.RECV_ID AS "RECV_ID"
			,T.REG_DTTM AS "REG_DTTM"
			,T.CONFIRM_DTTM AS "CONFIRM_DTTM"
			,T.DOCNUMBER AS "DOCNUMBER"
			,REPLACE(T.VERSION, '.0', '') AS "VERSION"
			,T.TITLE AS "TITLE"
			,T.END_DTTM AS "END_DTTM"
			,T.REG_DEPT_NAME AS "REG_DEPT_NAME"
			,T.REG_DEPT_ID AS "REG_DEPT_ID"
			,T.REG_USER_ID AS "REG_USER_ID"
			,T.REG_USER_NAME AS "REG_USER_NAME"
			,P.NAME_KOR AS "FULLPATH_NM"
		FROM (
			SELECT 
				M.WORK_SEQ
				,M.TASK_SEQ
				,M.RECV_ID
				,M.REG_DTTM
				,M.CONFIRM_DTTM
				,N.DOCNUMBER
				,N.VERSION
				,N.TITLE
				,N.END_DTTM
				,N.REG_DEPT_NAME
				,N.REG_DEPT_ID
				,N.REG_USER_ID
				,N.REG_USER_NAME
				,SPLIT_PART(O.FULLPATH, '|', 2)::INT AS P_NODE_ID
			FROM (
				SELECT
					A.WORK_SEQ
					,A.TASK_SEQ
					,B.RECV_ID
					,B.REG_DTTM
					,B.CONFIRM_DTTM
				FROM WF_DRAFT A
				INNER JOIN WF_DRAFT_RECEIVE B ON A.DRAFT_ID = B.DRAFT_ID
			WHERE B.USER_ID = '77471'
				AND B.STATUS = '0'
			) M
			left JOIN WB_TOTALBUILDERDATA_MDCS N ON M.WORK_SEQ = N.WORK_SEQ AND M.TASK_SEQ = N.TASK_SEQ
			left JOIN FR_CATEGORY_MAP O ON N.NODE_ID = O.NODE_ID
			WHERE 1 = 1
			
		) T
		left JOIN FR_CATEGORY_MAP P ON T.P_NODE_ID = P.NODE_ID
		WHERE 1 = 1
